{
  "name": "turborepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "ci": "turbo run build e2e:test api:test lint",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "storybook": "turbo run storybook"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest",
    "tailwindcss": "^3.2.4",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "prettier-plugin-tailwindcss": "^0.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.19.0"
}
